package Commands;

import Collection.*;
import Collection.OrganizationAsker;

public class add implements Commands{
    private CollectionManager cm;
    private OrganizationAsker ca;
    //в конструкторе создали два объекта - cm и ca
    public add(CollectionManager cm, OrganizationAsker ca){
        this.cm=cm;
        this.ca=ca;
    }
    //сейчас коллекшн менеджер добавляет орг аскера чтобы он создал орагнизацию
    public  execute(){
        cm.add(ca.createOrganization());
    }
}
